#include <stdio.h>
/*
Add `int max_of_four(int a, int b, int c, int d)` here.
*/
int max_of_four(int a, int b, int c, int d)
{   
    int k;
    k=(a>b&&a>c&&a>d)?a:b>c&&b>d?b:c>d?c:d;
    return k;
}



int main() {
    int a, b, c, d ;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    int k = max_of_four(a, b, c, d);
    printf("%d", k);
    
    return 0;
}

